{
  "name": "cta-tool",
  "version": "1.0.0",
  "description": "Core Tool for Compass Test Automation",
  "author": "CTA Team <cta@thomsonreuters.com>",
  "main": "lib/index.js",
  "scripts": {
    "test": "NODE_PATH=. _mocha",
    "codecoverage": "istanbul cover node_modules/mocha/bin/_mocha -- -R spec",
    "lint": "eslint --quiet lib test",
    "docs": "jsdoc --readme README.md lib -r -d docs && opener ./docs/index.html"
  },
  "repository": {
    "type": "git",
    "url": "git@git.sami.int.thomsonreuters.com:compass/cta-tool.git"
  },
  "engines": {
    "node": "6.9.4"
  },
  "devDependencies": {
    "babel-eslint": "~4.1.5",
    "chai": "~3.4.0",
    "eslint": "^3.2.2",
    "eslint-config-cta": "git+ssh://git@git.sami.int.thomsonreuters.com:compass/eslint-config-cta.git",
    "istanbul": "~0.4.1",
    "jsdoc": "~3.4.0",
    "jsdoc-to-markdown": "^1.3.6",
    "lodash": "^4.14.1",
    "mocha": "~2.3.4",
    "mock-require": "^1.3.0",
    "opener": "~1.4.0",
    "shortid": "^2.2.6",
    "sinon": "^1.17.5"
  },
  "dependencies": {
    "cta-logger": "git+ssh://git@git.sami.int.thomsonreuters.com:compass/cta-logger.git#1.0.0",
    "object-hash": "1.1.4"
  },
  "license": "Apache-2.0"
}
